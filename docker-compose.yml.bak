version: '3.8'

services:
  backend:
    build: ./AppMonitoringService.API
    ports:
      - "5000:80"
    environment:
      - ASPNETCORE_ENVIRONMENT=Development
      - ASPNETCORE_URLS=http://+:80

  frontend:
    build: ./AppMonitoringService.UI
    ports:
      - "4200:80"
    depends_on:
      - backend